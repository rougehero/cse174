import java.util.*;

public class shiftyCipher{

	public static void main(String[] args){
	
	String s = "";
	int len = msg.length();
	for(int x = 0; x < len; x++) {
	char c = (char)(msg.charAt(x) + shift);
	if (c > 'z')
		s += (char)(msg.charAt(x) - (26-shift));
		else
		s += (char)(msg.charAt(x) + shift);
	
	}
	}
	
}
