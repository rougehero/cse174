import java.util.*;

public class fillMatrix{

	public static void main(String[] args) {
	
	Scanner sc = new Scanner (System.in);
	
	System.out.print("How many rows in your matrix: ");
	int numRows = sc.nextInt();
	System.out.print ("How many columns in your matrix: ");
	int numCols = sc.nextInt();
	
	char[][] myMatrix = new char [numRows][numCols];
	System.out.println("Enter characters one on each line: ");
	String tempStr = "";
	char temp = 0;
	
	for(int i = 0; i < numRows; i++) {
		
		for(int k = 0; k < numCols; k++) {
			
			tempStr = sc.next();
			char letter = tempStr.charAt(0);
			myMatrix[i][k] = temp;
			}
			
		}
	
	for(int i = 0; i < numRows; i++) {
		
		for(int k = 0; k < numCols; k++) {
			
			System.out.print(myMatrix[i][k] + " ");
			}
			
		System.out.println("");
		
		}
	
	
	}

}
